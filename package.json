{
  "name": "jellyfin-ios",
  "version": "1.7.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "expo prebuild --platform ios",
    "lint": "eslint \".\"",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      "@jellyfin/ux-ios/.+\\.png$": "<rootDir>/__mocks__/assetFileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|normalize-url|axios|@jellyfin/sdk(/.*)?)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/metro.config.js",
      "!**/jest.setup.js"
    ],
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    "iOS > 11",
    "Android >= 5"
  ],
  "dependencies": {
    "@formatjs/intl-locale": "4.2.11",
    "@formatjs/intl-pluralrules": "5.4.4",
    "@jellyfin/sdk": "0.11.0",
    "@jellyfin/ux-ios": "1.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-menu/menu": "1.2.4",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.11.0",
    "@react-navigation/stack": "6.4.1",
    "axios": "1.9.0",
    "compare-versions": "3.6.0",
    "expo": "53.0.22",
    "expo-application": "~6.1.5",
    "expo-asset": "~11.1.7",
    "expo-av": "~15.1.7",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-keep-awake": "~14.1.4",
    "expo-localization": "~16.1.6",
    "expo-screen-orientation": "~8.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "i18next": "21.10.0",
    "normalize-url": "7.2.0",
    "prop-types": "15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "11.18.6",
    "react-lifecycles-compat": "3.0.4",
    "react-native": "0.79.5",
    "react-native-elements": "3.4.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "1.3.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "uuid": "8.3.2",
    "zustand": "5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "7.26.8",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/jest-native": "4.0.13",
    "@testing-library/react": "16.1.0",
    "@testing-library/react-native": "7.2.0",
    "@types/jest": "27.5.2",
    "@types/react": "~19.1.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "@typescript-eslint/parser": "8.42.0",
    "babel-preset-expo": "~12.0.0",
    "domexception": "4.0.0",
    "eslint": "8.57.1",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-native": "5.0.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "~53.0.9",
    "jest-fetch-mock": "3.0.3",
    "node-abort-controller": "3.1.1",
    "ts-jest": "26.5.6",
    "typescript": "~5.3.3"
  },
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
